    "@sveltepress/theme-default": "workspace:*",
    "@sveltepress/twoslash": "workspace:*",
    "@sveltepress/vite": "workspace:*",
    "@types/node": "^20.11.20",
    "svelte": "5.0.0-next.158",
    "svelte-check": "^3.6.4",
    "vite": "^5.1.4",
    "workbox-window": "^7.0.0"
  }
}
